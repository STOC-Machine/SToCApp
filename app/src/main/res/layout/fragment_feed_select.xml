<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:context=".FeedSelect" android:id="@+id/frameLayout2">

    <Button
            android:text="@string/feed_1_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/feedButton1" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="@string/feed_2_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/feedButton2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/feedButton1"/>
    <Button
            android:text="@string/feed_3_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/feedButton3" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/feedButton2"/>
    <Button
            android:text="@string/feed_4_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/feedButton4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/feedButton3"/>
    <Button
            android:text="Socket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="161dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="162dp" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/feedButton4"/>

</android.support.constraint.ConstraintLayout>