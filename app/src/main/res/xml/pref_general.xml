<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- NOTE: EditTextPreference accepts EditText attributes. -->
    <!-- NOTE: EditTextPreference's summary should be set to its value by the activity code. -->
    <EditTextPreference
            android:key="feed_ip_1"
            android:title="@string/setting_feed_1"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <EditTextPreference
            android:key="feed_ip_2"
            android:title="@string/setting_feed_2"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <EditTextPreference
            android:key="feed_ip_3"
            android:title="@string/setting_feed_3"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <EditTextPreference
            android:key="feed_ip_4"
            android:title="@string/setting_feed_4"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <EditTextPreference
            android:key="feed_ip_5"
            android:title="@string/setting_feed_5"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <EditTextPreference
            android:key="feed_ip_6"
            android:title="@string/setting_feed_6"
            android:selectAllOnFocus="true"
            android:inputType="number|text"
    />
    <ListPreference
            android:key="feed_refresh"
            android:title="@string/feed_refresh_rate"
            android:entries="@array/feed_refresh_options"
            android:entryValues="@array/feed_refresh_options"
            android:defaultValue="25000"
    />
    <MultiSelectListPreference
            android:key="feed_select"
            android:title="@string/setting_feed_select"
            android:entries="@array/feed_selection"
            android:entryValues="@array/feed_selection"
    />
</PreferenceScreen>
